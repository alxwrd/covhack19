{
  "name": "tfjs-examples-webcam-transfer-learning",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "vega-embed": "^3.0.0"
  },
  "scripts": {
    "start": "npm run copy-assets && cross-env NODE_ENV=development parcel index.html --no-hmr --open",
    "build": "npm run copy-assets && cross-env NODE_ENV=production parcel build index.html  --no-minify --public-url ./",
    "copy-assets": "rm -rf dist/ && mkdir dist && cp -R ./images ./dist/images && cp options.json ./dist/",
    "publish": "npm run build && gh-pages -d dist",
    "link-local": "yalc link"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.1.6",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "~1.10.3",
    "yalc": "~1.0.0-pre.22"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "ios >= 10",
    "android >= 6"
  ]
}
